{
  "name": "api",
  "version": "0.1.0",
  "scripts": {
    "dev": "uv run uvicorn agent_quant_api.main:app --reload --host 0.0.0.0 --port 8000",
    "start": "uv run uvicorn agent_quant_api.main:app --host 0.0.0.0 --port 8000",
    "lint": "uv run ruff check . && uv run ruff format --check .",
    "lint:fix": "uv run ruff check . --fix && uv run ruff format .",
    "format": "uv run ruff format .",
    "build": "uv sync --frozen",
    "check-types": "uv run mypy .",
    "test": "uv run pytest"
  }
}